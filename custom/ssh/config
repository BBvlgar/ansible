Host *
  User bamboo-compliance
  ForwardAgent yes
  StrictHostKeyChecking no



## Munich Servers
Host hq
  Hostname hq1-muc.it-economics.de
  User bamboo-compliance
  Port 60022
  IdentityFile ~/.ssh/id_rsa
  ControlMaster auto
  ControlPath ~/.ssh/ansible-%r@%h:%p
  ControlPersist 5m

Host jumphost
  Hostname hq1-muc.it-economics.de
  User bamboo-compliance
  Port 60022
  IdentityFile ~/.ssh/id_rsa
  ControlMaster auto
  ControlPath ~/.ssh/ansible-%r@%h:%p
  ControlPersist 5m

Host admin.muc.it-economics.de
     HostName admin.muc.it-economics.de
     Port 60022
     ForwardAgent yes
     ProxyCommand ssh -W %h:%p hq

